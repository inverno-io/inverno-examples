io.inverno.example.app_web.app_webConfiguration.web {
	http_server {
		h2c_enabled = true
		
		[ profile = "https" ] {
			h2c_enabled = false
			h2_enabled = true
			server_port = 8443
			tls_enabled = true
			key_store = "module:/keystore.jks"
			key_alias = "selfsigned"
			key_store_password = "password"
		}
	}
}
